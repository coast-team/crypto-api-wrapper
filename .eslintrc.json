{
  "extends": "conaclos/esnext-style",
  "rules": {
    "max-len": [2, 120, 4, {
      "ignoreUrls": true,
      "ignoreStrings": true,
      "ignoreTemplateLiterals": true,
      "ignoreRegExpLiterals": true
    }]
  },
  "parserOptions": {
    "ecmaVersion": 2017,
    "sourceType": "module"
  },
  "env": {
    "browser": true,
    "jasmine": true
  }
}
